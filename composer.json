{
  "name": "joshuapack/laravel-cloudflare",
  "description": "Use the Cloudflare API for Laravel 10, 11, and 12 using PHP >= 8.1.",
  "homepage": "https://github.com/joshuapack/laravel-cloudflare",
  "keywords": [
    "cloudflare",
    "laravel",
    "api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Joshua Pack",
      "email": "joshua@joshuapack.com",
      "homepage": "https://joshuapack.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.1",
    "cloudflare/sdk": "^1.3",
    "illuminate/contracts": "^10|^11|^12"
  },
  "autoload": {
    "psr-4": {
      "Joshuapack\\Cloudflare\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "aliases":{
        "Cloudflare": "Joshuapack\\Cloudflare\\CloudflareFacade"
      },
      "providers": [
        "Joshuapack\\Cloudflare\\CloudflareServiceProvider"
      ]
    }
  }
}
